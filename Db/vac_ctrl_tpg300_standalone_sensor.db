alias("$(CONTROLLERNAME):$(SENSOR):PrsR", "$(DEVICENAME):PrsR")

alias("$(CONTROLLERNAME):$(SENSOR):PrsStatR", "$(DEVICENAME):PrsStatR")

alias("$(CONTROLLERNAME):$(SENSOR):GaugeStatR", "$(DEVICENAME):GaugeStatR")

alias("$(CONTROLLERNAME):$(SENSOR):CtrlModeS", "$(DEVICENAME):CtrlModeS")

alias("$(CONTROLLERNAME):$(SENSOR):CtrlMode-RB", "$(DEVICENAME):CtrlMode-RB")

alias("$(CONTROLLERNAME):$(SENSOR):FltTimS", "$(DEVICENAME):FltTimS")

alias("$(CONTROLLERNAME):$(SENSOR):FltTim-RB", "$(DEVICENAME):FltTim-RB")

record("*", "${CONTROLLERNAME}:${SENSOR}:DevNameR")
{
    field(PINI, "YES")
    field(VAL,  "${DEVICENAME}")
}

record("*", "${CONTROLLERNAME}:${SENSOR}:NumOfRlysR")
{
