alias("${CONTROLLERNAME}:${SENSOR}:ValidR",           "${DEVICENAME}:ValidR")

alias("${CONTROLLERNAME}:${SENSOR}:ChanR",            "${DEVICENAME}:ChanR")

alias("${CONTROLLERNAME}:${SENSOR}:SensorTypeR",      "${DEVICENAME}:SensorTypeR")

alias("${CONTROLLERNAME}:${SENSOR}:PrsR",             "${DEVICENAME}:PrsR")
alias("${CONTROLLERNAME}:${SENSOR}:PrsR-STR",         "${DEVICENAME}:PrsR-STR")

alias("${CONTROLLERNAME}:${SENSOR}:PrsStatR",         "${DEVICENAME}:PrsStatR")

alias("${CONTROLLERNAME}:${SENSOR}:GaugeStatR",       "${DEVICENAME}:GaugeStatR")

alias("${CONTROLLERNAME}:${SENSOR}:CtrlModeS",        "${DEVICENAME}:CtrlModeS")

alias("${CONTROLLERNAME}:${SENSOR}:CtrlModeS-ErrorR", "${DEVICENAME}:CtrlModeS-ErrorR")

alias("${CONTROLLERNAME}:${SENSOR}:CtrlMode-RB",      "${DEVICENAME}:CtrlMode-RB")

alias("${CONTROLLERNAME}:${SENSOR}:FltTimS",          "${DEVICENAME}:FltTimS")

alias("${CONTROLLERNAME}:${SENSOR}:FltTim-RB",        "${DEVICENAME}:FltTim-RB")


record("*", "${CONTROLLERNAME}:${SENSOR}:DevNameR")
{
  field(PINI, "YES")
  field(VAL,  "${DEVICENAME}")
}


record("*", "${CONTROLLERNAME}:${SENSOR}:NumOfRlysR")
{
  field(VAL,  "2")
  field(PINI, "YES")
}


record("*", "${CONTROLLERNAME}:${SENSOR}:iCheckModType")
{
  field(CALC, "(A&&(AA[0,1]=='${BOARD}'||AA[0,1]=='CP'))?'1':'0'")
}


record(stringin, "${CONTROLLERNAME}:${SENSOR}:iSensorTypeR")
{
  field(DISP, "1")
  field(PINI, "YES")
  field(VAL,  "${GAUGE}")
}
